<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.newyulong.materialdesignstu">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TranslucentTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextInputLayoutActivity"/>
        <activity android:name=".DrawerLayoutActivity"/>
        <activity android:name=".ToolBarActivity"/>
        <activity android:name=".SnackbarActivity"/>
        <activity android:name=".FloatingActionButtonActivity"/>
        <activity android:name=".TabLayoutActivity"/>
        <activity android:name=".CoordinatorLayoutActivity"/>
        <activity android:name=".AppBarLayoutActivity"/>
        <activity android:name=".CollapsingToolbarLayoutActivity"/>
        <activity android:name=".RecyclerViewActivity"/>
        <activity android:name=".ViewAndAppbarLayout"/>
        <activity android:name=".PicassoActivity"/>
        <activity android:name=".RxJavaActivity"/>
        <activity android:name=".RetrofitActivity"/>
    </application>

</manifest>